<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TalkingPaper" namespace="TalkingPaper">
  <class name="TalkingPaper.Contenuto, TalkingPaper" table ="Contenuto" lazy="true">
    <id name="IDcontenuto" column ="IDcontenuto" type="int" unsaved-value="0">
      <generator class="native"/>
    </id>
    <property name="Livello" column="Livello" type="int" not-null="true" />
    <property name="Ordine" column="Ordine" type="int" not-null ="true" />
    <property name="Barcode" column="Barcode" type ="string" not-null="false"/>
    <property name="Rfidtag" column="Rfidtag" type="string" not-null="false" />
    <property name="Nome" column="Nome" type="string" not-null="true"/>
    <!-- E' in lato 1 in relazione con Poster, tipologia e risorsamultimediale-->
    <!--
    <many-to-one name="Poster" class="Poster" column="IDposter" not-null="false" lazy="false" />
    <many-to-one name="Tipo" class="Tipologia" column="IDtipologia" not-null="false" lazy="false"/>
    <many-to-one name="RisorsaMultimediale" class="Risorsamultimediale" column="IDrisorsa" not-null="false" lazy="false"/>
  -->
    <many-to-one name="Poster" class="Poster" column="Poster" not-null="false" lazy="false" />
    <many-to-one name="Tipo" class="Tipologia" column="Tipo" not-null="false" lazy="false"/>
    <many-to-one name="RisorsaMultimediale" class="Risorsamultimediale" column="RisorsaMultimediale" not-null="false" lazy="false"/>
    <!-- Partecipa con Altrarisorsa ad una relazione N:M-->
    <bag name="AltrarisorsaLista" table="altra_contenuto">
      <key column="IDcontenuto"/>
      <many-to-many column="IDaltra" class="TalkingPaper.Altrarisorsa, TalkingPaper"/>
    </bag>
  
  
  </class>
</hibernate-mapping>

