<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TalkingPaper" namespace="TalkingPaper">
  <class name="TalkingPaper.Utente, TalkingPaper" table="Utente" lazy="true">
    <id name="IDutente" column ="IDutente" type="int" unsaved-value="0">
      <generator class="native"/>
    </id>
    <property name="Nome" column="Nome" type="string" not-null="true" />
    <property name="Cognome" column="Cognome" type="string" not-null ="true" />
    <property name="Classe" column="Classe" type ="string" not-null="false"/>
    <property name="AnnoNascita" column="AnnoNascita" type="DateTime" not-null="false"/>
    <property name="Barcode" column="Barcode" type="string" not-null="false"/>
    <property name="Rfidtag" column="Rfidtag" type="string" not-null="false"/>
    <!-- E' in lato 1 in relazione con Mostra -->
    <many-to-one name="Profilo" column="Profilo" not-null="false" lazy="false"/>
  </class>
</hibernate-mapping>
