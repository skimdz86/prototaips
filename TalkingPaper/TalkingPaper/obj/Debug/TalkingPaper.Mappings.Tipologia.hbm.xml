<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TalkingPaper" namespace="TalkingPaper">
  <class name="TalkingPaper.Tipologia, TalkingPaper" table ="Tipologia" lazy="true">
    <id name="IDtipologia" column ="IDtipologia" type="int" unsaved-value="0">
      <generator class="native"/>
    </id>
    <property name="Descrizione" column="Descrizione" type="string" not-null="true" />
    <property name="Tipo" column="Tipo" type="string" not-null ="true" />
    <bag name ="ContenutoLista" cascade ="all" lazy="true" inverse="true">
      <key column="Tipo" />
      <one-to-many class ="TalkingPaper.Contenuto, TalkingPaper"/>
    </bag>
  </class>
</hibernate-mapping>
