<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TalkingPaper" namespace="TalkingPaper">
  <class name="TalkingPaper.Altrarisorsa, TalkingPaper" table ="Altrarisorsa" lazy="true">
    <id name="IDaltra" column ="IDaltra" type="int" unsaved-value="0">
      <generator class="native"/>
    </id>
    <property name="Nome" column="Nome" type="string" not-null="true" />
    <property name="Path" column="Path" type="string" not-null ="true" />
    <property name="Tipo" column="Tipo" type ="string" not-null="true" />
    <!-- Bag per essere in relazione N:M con Profilo -->
    <bag name="ContenutoLista" table="altra_contenuto">
      <key column="IDaltra"/>
      <many-to-many column="IDcontenuto" class="TalkingPaper.Contenuto, TalkingPaper"/>
    </bag>
  </class>
</hibernate-mapping>

